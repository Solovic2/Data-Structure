public class Fabion {
 public static int fabi(int n){
             int f1=0,f2=1;
             if(n==1){
         return 1;
     }else if(n==0){
         return 0;
     }else{
         for(int i=0;i<n;i++){
           int f3=f2;
             f2=f2+f1;
             f1=f3;
         }
         return f1;
     }
    }
   public static void main(String args[]) {
     Scanner mo =new Scanner(System.in);

     int n=mo.nextInt();
     int g = fabi(n);
     system.out.print(g);
    }
}
